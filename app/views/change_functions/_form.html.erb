<%= form_with(model: change_function, local: true) do |form| %>
  <% if change_function.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(change_function.errors.count, "error") %> prohibited this change_function from being saved:</h2>

      <ul>
      <% change_function.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :branch_id %>
    <%= form.text_field :branch_id %>
  </div>

  <div class="field">
