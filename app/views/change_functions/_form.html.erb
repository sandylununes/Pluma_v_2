    <%= form.text_field :collaborator %>
  </div>

  <div class="field">
    <%= form.label :cost_center_id %>
    <%= form.text_field :cost_center_id %>
  </div>

  <div class="field">
    <%= form.label :current_position %>
    <%= form.text_field :current_position %>
  </div>

  <div class="field">
    <%= form.label :new_position %>
    <%= form.text_field :new_position %>
  </div>

  <div class="field">
    <%= form.label :date_change %>
    <%= form.date_select :date_change %>
  </div>

  <div class="field">
    <%= form.label :status %>
    <%= form.number_field :status %>
  </div>

  <div class="field">
    <%= form.label :user_id %>
    <%= form.text_field :user_id %>
  </div>

  <div class="actions">
